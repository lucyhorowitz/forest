	\arrow[shorten <=19pt, shorten >=19pt, equals, from=1-4, to=1-6]
	\arrow[""{name=2, anchor=center, inner sep=0}, "R", curve={height=-12pt}, from=1-6, to=1-8]
	\arrow[""{name=3, anchor=center, inner sep=0}, "R"', curve={height=12pt}, from=1-6, to=1-8]
	\arrow["\epsilon"{pos=0.4}, shorten >=2pt, Rightarrow, from=1-2, to=0]
