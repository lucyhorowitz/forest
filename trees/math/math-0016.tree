\title{zig-zag law}
\import{macros}
\import{base-macros}
\import{rel}
\taxon{Identity}

\p{The following pairs of 2-morphisms are actually equal: 
\quiver{% https://q.uiver.app/#
\begin{tikzcd}
	C & D & C & D && C && D
	\arrow["L", from=1-1, to=1-2]
	\arrow[""{name=0, anchor=center, inner sep=0}, "{\rm{id}}", curve={height=-24pt}, from=1-1, to=1-3]
	\arrow["R", from=1-2, to=1-3]
	\arrow[""{name=1, anchor=center, inner sep=0}, "{\rm{id}}"', curve={height=24pt}, from=1-2, to=1-4]
	\arrow["L", from=1-3, to=1-4]
	\arrow[shorten <=19pt, shorten >=19pt, equals, from=1-4, to=1-6]
	\arrow[""{name=2, anchor=center, inner sep=0}, "L"', curve={height=12pt}, from=1-6, to=1-8]
	\arrow[""{name=3, anchor=center, inner sep=0}, "L", curve={height=-12pt}, from=1-6, to=1-8]
	\arrow["\eta", shorten <=2pt, Rightarrow, from=0, to=1-2]
	\arrow["\epsilon"{pos=0.3}, shorten >=2pt, Rightarrow, from=1-3, to=1]
	\arrow["{\rm{id}}", shorten <=3pt, shorten >=3pt, Rightarrow, from=3, to=2]
\end{tikzcd}}
\quiver{% https://q.uiver.app/#
\begin{tikzcd}
	D & C & D & C && D && C
	\arrow["R", from=1-1, to=1-2]
	\arrow[""{name=0, anchor=center, inner sep=0}, "{\rm{id}}"', curve={height=24pt}, from=1-1, to=1-3]
	\arrow["L", from=1-2, to=1-3]
	\arrow[""{name=1, anchor=center, inner sep=0}, "{\rm{id}}", curve={height=-24pt}, from=1-2, to=1-4]
