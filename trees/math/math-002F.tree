	\arrow["{\alpha\circ(1_{C_1}\times_{C_0}\langle et,1_{C_1}\rangle)}", from=1-1, to=1-3]
	\arrow["{c\circ (1_{C_1}\times_{C_0}\lambda)}"', from=1-1, to=2-2]
	\arrow["{c\circ(\rho\times_{C_0}1_{C_1})}", from=1-3, to=2-2]
\end{tikzcd}}
